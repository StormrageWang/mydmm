<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF"> 
  <appenders> 
    <Console name="Console" target="SYSTEM_OUT"> 
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5level [%t] - %m%n"/> 
      <!-- %C{1}.%M:%L -->
    </Console> 
  </appenders> 
  <loggers> 
    <root level="trace"> 
      <appender-ref ref="Console"/> 
    </root> 
  </loggers> 
</configuration>
