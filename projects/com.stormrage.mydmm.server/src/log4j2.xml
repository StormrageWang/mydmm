<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF"> 
  <appenders> 
    <Console name="ConsoleDetail" target="SYSTEM_OUT"> 
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5level [%t] - %m%n"/> 
    </Console>
	<File name="FileDetail" fileName="e:/logs/mydmm.log" append="false">
		<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5level [%t] - %m%n" />
	</File>
  </appenders> 
  <loggers> 
    <root level="debug"> 
      <appender-ref ref="ConsoleDetail"/> 
    </root> 
  </loggers> 
</configuration>
